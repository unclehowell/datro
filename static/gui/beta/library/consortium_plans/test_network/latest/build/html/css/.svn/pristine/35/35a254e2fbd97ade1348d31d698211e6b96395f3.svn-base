@media all and (min-width: 768px) and (max-width: 959px) {
}

@media all and (min-width: 480px)and (max-width: 767px) {
	.paragraph-content {font-size: 125%!important; padding-left: 1.5rem; padding-right: 1.5rem; word-spacing:0.2rem;}
	.main-image {max-height: calc(30vh);}
}

@media all and (max-width: 479px) {
	.main-image {max-height: calc(30vh);}
}

