.gw-repo-outer {
  float: left;
  width: 50%;
}
.gw-repo-outer:nth-child(2n + 1) {
  clear: left;
}
.gw-repo {
  padding: 0.5em 0;
  margin: 0 1.5em 1em 0;
  border-top: 1px solid #ccc;
}
.gw-repo:nth-child(2n) {
  margin: 0 0 1em 1.5em;
}
.gw-title{
  margin-bottom: 0.3em;
}
.gw-name {
  font-weight: bold;
}
.gw-stats {
  display: inline-block;
  margin: 0;
  float: right;
}
.gw-watchers, .gw-forks {
  display: inline-block;
  height: 21px;
  padding: 0 5px 0 23px;
  line-height: 21px;
}
.gw-watchers {
  background-position: 5px -2px;
}
.gw-forks {
  background-position: 5px -52px;
}
.gw-lang {
  font-size: 0.8em;
  color: #777;
}
.gw-clearer {
  clear: left;
}

.dev-cal {text-align:center; display: block; margin-left: auto; margin-bottom:5rem; margin-right: auto; zoom:1.1; float:right; min-width: 100vw; }
.dev-cal-div-top, .dev-cal-div{  width: inherit;
  height: inherit;
  position: inherit;
  top: 0;
  height: inherit;
  left: 0; border:1px solid blue; margin-left:-10vw; }
 
 .dev-cal-div { left: 0;  z-index: 10; border:1px solid red; height: 100px;}
@media (max-width: 800px) and (orientation: portrait) {
	 .gw-repo-outer { margin-left: 10vw;float:unset!important; width:unset!important;zoom:0.83;}
	
	 }
	 
	 	.gw-stats> { height:20px!important; }
		.gw-stats>li { border:1px solid grey; display:inline!Important;  }
		
		.gw-forks { margin: 0px 2px 0 8px!important; padding: 1px 15px 0 63px;
					line-height: 26px;
					background: transparent url(../img/fork.png) no-repeat 0 0;
					background-size: initial;
					background-position: center;
					align-content: right!important;
					align-items: right!important;
					text-align: -webkit-right!important;
					}
		
		.gw-watchers {margin: 0px 2px 0 30px!important;
					padding: 1px 12px 0 77px;
					line-height: 26px;
					background: transparent url(../img/watcher.png) no-repeat 0 0;
					background-size: initial;
					background-position: center;
					align-content: right!important;
					align-items: right!important;
					text-align: -webkit-right!important;
					}

		* {box-sizing: border-box}
	