/* 01:> */
/* 01:A */ .container {left:calc(10vw); padding-right:calc(10vw); padding-left: 15px; padding-right: 15px; padding-top: 15px; bottom:-50px;}
/* 01:B */ .carousel-maintitle {font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"; font-weight: 300!important; color:white!important; opacity:1; margin-bottom:0.6rem; font-size:1.25rem;}
/* 01:C */ .carousel-maintitle-link {opacity: 1; color:white; text-decoration:none; letter-spacing:0.03rem; text-decoration:none; font-weight:300;}
/* 01:D */ .carousel-maintitle-link:hover {color:white; text-decoration:none; font-weight:300;}
/* 01:E */ .carousel-title {font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"; font-weight: 300; color:#61CFF7; opacity:1; font-size:1rem;}
/* 01:F */ .carousel-title-link {opacity: 1; color:#61CFF7; text-decoration:none; letter-spacing:0.03rem;}
/* 01:G */ .carousel-title-link:hover {color:#A0E1FA; text-decoration:none;}
/* 01:H */ .carousel-subtitle {text-indent: 0px; display:inline-block!important; color: #CCCCCC; text-indent: 0px; font-weight:300!important; font-size:1rem;}
/* 01:I */ .carousel-item {height: 32rem;}
/* 01:J */ .appstore-img-button-play { background-image:url('../img/google-play-66442243.svg'); background-repeat: no-repeat; background-position: 0px 0px; display: inline-block; background-color: transparent; padding-left: 0px; margin-top: 0px; height:42px; width: 110px; float: left;}
/* 01:K */ .appstore-img-button-apple {background-image:url('../img/app-store-1f36bbd8.svg'); background-repeat: no-repeat; background-position: 0px 0px; display: block; background-color: transparent; padding-left: 0px; margin-top: -1px; margin-left: 10px; height:42px; width: 110px; float: left;}
/* 01:L */ .payment-button-img-paypal {background-image:url('../img/paypal-logo.png'); background-repeat: no-repeat; background-position: 3px 2.5px; background-size: 82px auto; margin-left: 10px; display:inline-block!important; border: 0.05rem solid white; background-color: transparent; border-radius:4px 4px 4px 4px; width: 110px; height: 32px;}
/* 01:M */ .button-text-paypal {font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"; font-weight: 300!important; color: white; letter-spacing:1px; text-align:left; margin-left:40px; font-size: 1rem; margin-top:3px; float:left;}
/* 01:N */ .payment-button-img-stripe {background-image:url('../img/stripe-logo.png'); background-repeat: no-repeat; background-position: 3px 2.5px; background-size: 82px auto; display:inline-block!important; margin-left: 2.5px; border-left: 0.2rem solid #343A40; border: 0.05rem solid white; background-color:transparent!Important; border-radius:4px 4px 4px 4px; cursor: pointer; width: 110px; height: 32px;}
/* 01:O */ .button-text-stripe {font-family: -apple-system,BlinkMacSystemFont,"Segoe UI",Roboto,"Helvetica Neue",Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"; font-weight: 300!important; color: white; letter-spacing:1px; text-align:left; margin-left:40px; font-size: 1rem; margin-top:-2.5px; float:left;}
/* 01:P */ .button-block {display:inline-block; overflow:visible!important; height: 50px; width:380px; letter-spacing:30px; position:static; padding-top: 1.25rem; padding-bottom: 4rem!important;}
/* 01:Q */ .button-block-left {margin-left: 0px; float: left;}
/* 01:R */ .button-block-right {margin-left: 0px; float: left;}
/* 01:S */ .tick-box-holder {text-decoration: underline; color: #CCCCCC;}
/* 01:T */ .tick-box {opacity:0.6; background-color: transparent; border:2px solid white; border-radius:4px; width:18px; height:18px; display:inline-block;}
/* 01:U */ .moreinfo {padding-bottom:3px; opacity:0.8; margin-left:9px; width:1.25rem; height:auto; cursor: pointer;}
/* 01:V */ #image1>img,#image2>img,#image3>img,#image4>img,#image4>img {    max-width: calc(75vw);
    height: auto;
    min-height: calc(30vw);
    max-height: 280px;
    margin-left: calc(12.5vw);
    margin-top: 75px;
    transition: width 0.1s ease-out;
    padding-bottom: 0!important;
    margin-bottom: 0!important;}
/* 01 ! */

