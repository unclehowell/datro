/* 01:A */	body {background-color: #333366; color: white; text-align:left; padding-left:20px; padding-right: 20px;}
/* 01:B */ .ui-draggable, .ui-droppable {background-position: top;}
/* 01:C */	p {line-height: 6px;font-family:Helvetica,Arial,sans-serif;}
/* 01:D */	h1 {margin-top:20px;text-align:center;}
/* 01:E */	a {font-family: Helvetica,Arial,sans-serif; margin-top: -5px; text-decoration: none; color: transparent!important; border-radius: 0px; border: 0.1px solid transparent!important; padding-top: 0px; background-color: transparent!important; height: 1px; width: 1px; float: left;}
/* 01:F */	iframe {margin-top: 20px; width: 100%; text-align: center; height: calc(70vh); border: 1px solid white; opacity: 0.8; }
/* 01:G */ .territory-title {left: 0; padding-left:0.5rem; float: left; font-size: 1.5rem; font-weight:lighter; text-transform:uppercase;}
/* 01:H */ .territory-back-img {opacity:0; background-color: none; border:0; margin-right: 10px; height: 40px; float: left; }
/* 01:I */ .territory-flag-img {margin-right: 10px; height: 50px; float: center; margin-bottom: -15px;}
/* 01:J */ .territory-iframe-div {width:100%; overflow: display; text-align:center;}
/* 01:K */ .territory-iframe {max-width: 914px;}
/* 01 ! */ 

