.currencydrop dd ul::-webkit-scrollbar-track {-webkit-box-shadow: inset 0 0 1px rgba(0,0,0,0.3); border-left:1px solid rgba(0,0,0,0.1);}
.currencydrop dd ul::-webkit-scrollbar-thumb { background: rgba(0,0,0,0.4);}
.currencydrop dd ul::-webkit-scrollbar-thumb:window-inactive {background: blue;}
.currencydrop span.value {display: none;}
.currencydrop dd ul li a {padding: 5px; display: block; font-size: 12px !important;}
.currencydrop dd ul li a:hover {background-color: rgba(0,0,0,0.05);}
.currencydrop li a {font-size:13px;}
.currencydrop li a span:nth-child(2) {line-height: 2em; color:#61CFF7;}
dl.currencydrop {display: inline-block; width: 100px; margin: -18px 0 0 1px; float:left;}
dl.currencydrop > span:nth-child(3) {color: rgba(0,0,0,0.4)}
dl.currencydrop dt span:nth-child(3) {display: none;}
dl.currencydrop > span:nth-child(2) {overflow: hidden; white-space: nowrap; display: inline-block;}
dl.currencydrop dt span:nth-child(2) {color: #61CFF7; font-size: 0.75rem; font-weight: 300; margin-top: 0.25rem; background-color: transparent; position: absolute; margin-left: -0.1px;}
.countryFlag {padding: 0; background-image: url("https://i.imgur.com/OQiDoZe.png"); background-repeat: no-repeat; display: inline-block; height: 11px; margin-right: 4px; width: 16px; cursor: pointer; white-space: nowrap; -moz-border-bottom-colors: none; -moz-border-left-colors: none; -moz-border-right-colors: none; -moz-border-top-colors: none; border-color: #BFBFC1 #B6B6B6 #969696; border-image: none; border-radius: 2px 2px 2px 2px; border-style: solid; border-width: 1px; box-shadow: 0 1px 1px rgba(0, 0, 0, 0.09);}
dl.currencydrop>img {display:hidden; display:none;}
.currencydrop dt a:after {content: ''; background: transparent; position: absolute; height: 22px; position: absolute; right: 0; top: 0; width: 35px;}
.currencydrop dt a:before {display:hidden;}
/* background: none repeat scroll 0 0 #FFFFFF; content: ""; height: 3px; position: absolute; right: 7px; top: 6px; width: 20px; z-index: 2; box-shadow:0 8px 0 #FFFFFF, 0 16px 0 #FFFFFF;} */


@media all and (min-width: 1024px) {
	.carousel-dropdown a, .carousel-dropdown a:visited {font-size:0.95rem!important;}
	.carousel-dropdown dt a span {font-size:1.01rem!important;}
}
